<div class="sixteen">
	<h3><%= @title %></h3>
		<% @album[:photos].sort_by!{ |photo| photo[:time] }.reverse.each_with_index do |photo, i|%>
		<article>
			<h5><a href="<%= 'https://plus.google.com/photos/'+ @name +'/albums/'+ url +'/'+photo[:id]%>"> <%= photo[:title] %> </a></h5>
			<a href="<%= PicasaAPI::url_for_dimension(photo[:src], "720") %>" rel="lightbox[blog]"><img src='<%= PicasaAPI::url_for_dimension(photo[:src], "600") %>' alt=" <%= photo[:title] %>"></a>
		</article>
	<% end %>
</div>